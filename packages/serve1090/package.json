  "license": "AGPL-3.0-only",
  "author": "Robert Steilberg <rsteilberg@gmail.com>",
  "scripts": {
    "start": "node index.js",
    "pm2": "pm2 index.js",
    "debug": "nodemon --inspect=0.0.0.0 index.js",
    "d": "npm run debug",
    "test": "jest",
    "t": "npm run test",
    "l": "docker logs --follow express",
