    "camelcase-keys": "^7.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-ws": "^5.0.2",
    "got": "^11.8.3",
    "ioredis": "^4.28.2",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.2.1",
    "mongodb-memory-server": "^8.0.3",
    "nanoid": "^3.1.30",
    "p-map": "^4.0.0",
