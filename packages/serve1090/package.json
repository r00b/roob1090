{
  "name": "serve1090",
  "version": "2.0.2",
  "private": true,
  "description": "WebSocket server for receiving dump1090 data.",
  "repository": "https://github.com/robertsteilberg/serve1090/",
  "license": "AGPLv3",
  "author": "Robert Steilberg <rsteilberg@gmail.com>",
  "scripts": {
    "start": "node index.js",
    "debug": "nodemon --inspect=0.0.0.0 index.js --ignore logs",
    "compose": "docker-compose up -d --build",
    "tail-logs": "docker logs --follow serve1090_express",
    "serve:debug": "yarn redis-restart && yarn debug",
    "redis": "docker run -v $PWD/keydb.conf:/etc/keydb/keydb.conf --publish=6379:6379 --name=serve1090_keydb -d eqalpha/keydb:latest",
    "redis-start": "docker start serve1090_keydb",
    "redis-stop": "docker stop serve1090_keydb",
    "redis-remove": "docker rm serve1090_keydb",
    "redis-restart": "yarn redis-stop && yarn redis-remove && yarn redis",
    "start-pm2": "pm2 start index.js"
  },
  "dependencies": {
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/distance": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "base-64": "^0.1.0",
    "bree": "^3.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "got": "^11.6.2",
    "ioredis": "^4.17.3",
    "joi": "^17.2.1",
    "lodash": "^4.17.20",
    "p-map": "^4.0.0",
    "pino": "^6.6.1",
    "pm2": "^4.4.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "signale": "^1.4.0"
  },
  "volta": {
    "node": "14.5.0",
    "yarn": "1.22.5"
  }
}
