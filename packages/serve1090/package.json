{
  "name": "serve1090",
  "version": "2.4.0",
  "description": "WebSocket server for receiving, aggregating, and serving dump1090 data",
  "repository": "https://github.com/robertsteilberg/serve1090/",
  "license": "AGPL-3.0-only",
  "author": "Robert Steilberg <rsteilberg@gmail.com>",
  "scripts": {
    "start": "node index.js",
    "pm2": "pm2-runtime index.js",
    "debug": "nodemon --inspect=0.0.0.0 index.js",
    "d": "npm run debug",
    "test": "jest",
    "t": "npm run test",
    "l": "docker logs --follow express",
    "cl": "docker-compose up -d --build && npm run l",
    "mongo-admin": "mongo-express -a --url mongodb://$(grep MONGO_USERNAME .env | cut -d '=' -f2):$(grep MONGO_PASSWORD .env | cut -d '=' -f2)@127.0.0.1:27017"
  },
  "dependencies": {
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "@turf/distance": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "bree": "^6.2.1",
    "camelcase-keys": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-ws": "^5.0.2",
    "got": "^11.8.2",
    "ioredis": "^4.27.6",
    "joi": "^17.4.1",
    "lodash": "^4.17.21",
    "mongodb": "^4.1.4",
    "mongodb-memory-server": "^8.0.3",
    "nanoid": "^3.1.23",
    "p-map": "^4.0.0",
    "pino": "^6.12.0",
    "pm2": "^5.1.0",
    "safe-compare": "^1.1.4"
  },
  "devDependencies": {
    "ioredis-mock": "^5.8.0",
    "jest": "^27.3.1",
    "mongo-express": "^1.0.0-alpha.4",
    "nock": "^13.2.1",
    "nodemon": "^2.0.12",
    "signale": "^1.4.0",
    "supertest": "^6.1.6",
    "superwstest": "^1.8.0"
  },
  "volta": {
    "node": "16.5.0",
    "npm": "7.19.1"
  }
}
